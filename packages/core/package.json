{
  "name": "@nimiq-auth/core",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.15.4",
  "description": "Add Login with Nimiq to your apps",
  "author": "onmax",
  "homepage": "https://github.com/onmax/nimiq-auth#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onmax/nimiq-auth.git"
  },
  "bugs": "https://github.com/onmax/nimiq-auth/issues",
  "keywords": [
    "nimiq",
    "auth",
    "core"
  ],
  "sideEffects": false,
  "exports": {
    "./server": {
      "types": "./dist/server.d.mts",
      "import": "./dist/server.mjs"
    },
    "./client": {
      "types": "./dist/client.d.mts",
      "import": "./dist/client.mjs"
    },
    "./challenge": {
      "types": "./dist/challenge.d.mts",
      "import": "./dist/challenge.mjs"
    },
    "./types": {
      "types": "./dist/types.d.mts",
      "import": "./dist/types.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nimiq/core": "^2.0.6",
    "@nimiq/hub-api": "^1.10.0",
    "uncrypto": "^0.1.3"
  },
  "devDependencies": {
    "tsx": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:"
  }
}
