{
  "name": "@nimiq-auth/core",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.15.4",
  "description": "Add Login with Nimiq to your apps",
  "author": "onmax",
  "homepage": "https://github.com/onmax/nimiq-auth#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onmax/nimiq-auth.git"
  },
  "bugs": "https://github.com/onmax/nimiq-auth/issues",
  "keywords": [
    "nimiq",
    "auth",
    "core"
  ],
  "sideEffects": false,
  "exports": {
    "./server": {
      "types": "./dist/server.d.mts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.cjs"
    },
    "./client": {
      "types": "./dist/client.d.mts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.cjs"
    },
    "./types": {
      "types": "./dist/types.d.mts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    },
    "./jwt": {
      "types": "./dist/jwt.d.mts",
      "import": "./dist/jwt.mjs",
      "require": "./dist/jwt.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nimiq/core": "catalog:",
    "@nimiq/hub-api": "^1.10.0",
    "crypto-browserify": "^3.12.1",
    "h3": "^1.14.0",
    "uncrypto": "^0.1.3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.8",
    "jsonwebtoken": "^9.0.2",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:"
  }
}
