{
  "name": "@nimiq-auth/nuxt",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.15.4",
  "description": "Add Login with Nimiq to your apps",
  "author": "onmax",
  "homepage": "https://github.com/onmax/nimiq-auth#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onmax/nimiq-auth.git"
  },
  "bugs": "https://github.com/onmax/nimiq-auth/issues",
  "keywords": [
    "nimiq",
    "auth"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "typecheck": "nuxi typecheck",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nimiq-auth/core": "workspace:*",
    "nuxt-auth-utils": "catalog:"
  },
  "devDependencies": {
    "@nimiq/hub-api": "^1.10.0",
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/kit": "^3.15.4",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/test-utils": "^3.15.4",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "defu": "catalog:",
    "nuxt": "catalog:",
    "typescript": "catalog:",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.4.1",
    "vitest": "catalog:",
    "vue-tsc": "^2.2.0"
  }
}
